runtime_dep: "kbd"
runtime_dep: "dosfstools"  # for mkfs.fat

install: {
  file: {
    srcpath: "pkg/mod/distri1@v0/zsh/site-functions/_distri"
    destpath: "share/zsh/site-functions/_distri"
  }

  file: {
    srcpath: "pkg/mod/distri1@v0/zsh/zshrc"
    destpath: "../etc/zshrc"
  }
}
