source: "distriroot://distri1-1.tar.gz"
hash: ""
version: "native-44"

gobuilder: {
  install: "./cmd/distri ./cmd/minitrd ./cmd/distri-installer"
  import_path: "distri1"
  # Used as /init before the dynamic linker is available:
  go_env: "CGO_ENABLED=0"
}

# build dependencies:
